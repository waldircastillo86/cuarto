#include <iostream>
using namespace std;
int main () {
    int mes;
    cout<<"ingrese un numero de mes (1-12): ";
    cin>>mes;
    if(mes==1){
        cout<<"enero"<<endl;
    }else if (mes==2) {
    cout<<"febrero" <<endl;
     }else if (mes==3) {
    cout<<"marzo" <<endl;
     }else if (mes==4) {
    cout<<"abril" <<endl;
     }else if (mes==5) {
    cout<<"mayo" <<endl;
     }else if (mes==6) {
    cout<<"junio" <<endl;
     }else if (mes==7) {
    cout<<"julio" <<endl;
     }else if (mes==8) {
    cout<<"agosto" <<endl;
     }else if (mes==9) {                           
    cout<<"septiembre" <<endl;
     }else if (mes==10) {
    cout<<"octubre" <<endl;
     }else if (mes==11) {
    cout<<"noviembre" <<endl;
     }else if (mes==12) {
    cout<<"diciembre" <<endl;
    }else {
    cout<<"numero de mes invalido"<<endl;
    }
    return 0;
}      MERCY ES LA MEJOR :)
