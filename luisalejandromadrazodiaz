#include <iostream>
#include <cmath>

// Función para calcular el cuadrado de un número
double square(double num) {
    return num * num;
}

int main() {
    double number;
    
    std::cout << "Introduce un número: ";            //MADRAZO LE GUSTA FERNANDA
    std::cin >> number;                                                                 
    
    // Calculamos el cuadrado del número
    double result = square(number);
    
    std::cout << "El cuadrado de " << number << " es " << result << std::endl;
    
    return 0;
}
